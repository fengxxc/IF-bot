  },
  "homepage": "https://github.com/fengxxc/IF-bot#readme",
  "dependencies": {
    "dotenv": "^16.0.1",
    "https-proxy-agent": "^5.0.1",
    "inkjs": "^2.1.0",
    "socks-proxy-agent": "^7.0.0",
    "telegraf": "^4.8.5",
    "telegraf-session-local": "^2.1.0"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
